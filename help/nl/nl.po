msgstr "brand"

#: C/mancreate.page:180(p)
msgid "Brand or manufacturer of stationery product. E.g. \"Avery\""
msgstr ""

#: C/mancreate.page:183(code)
#, no-wrap
msgid "part"
msgstr "part"

#: C/mancreate.page:184(p)
msgid "Part number or name of stationery product. E.g. \"8160\""
